        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ListOfCoordinates" Height="438" Width="427"
        Background="#3D5C5C">
  <Grid>
    <ScrollViewer HorizontalAlignment="Left" Width="auto" Height="auto" Margin="10,10,0,0" VerticalAlignment="Top">
      <TextBlock Name="listofcoord" Text="{Binding Message, Mode=TwoWay}" FontFamily="Plantagenet Cherokee" FontSize="14" Margin="35, 0, 0, 0" Width="365" Height="377"/>
    </ScrollViewer>
  </Grid>
</Window>
